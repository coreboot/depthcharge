# Arch
CONFIG_ARCH_X86=y

# Board
CONFIG_BOARD_DIR="samus"

# Image

# Vboot
CONFIG_EC_VBOOT_SUPPORT=y

CONFIG_CROSSYSTEM_ACPI=y
CONFIG_NVDATA_CMOS=y

# Kernel format
CONFIG_KERNEL_ZIMAGE=y

# Drivers
CONFIG_DRIVER_AHCI=y
CONFIG_DRIVER_BUS_I2C_DESIGNWARE=y
CONFIG_DRIVER_BUS_I2S_BROADWELL=y
CONFIG_DRIVER_BUS_I2S_BROADWELL_ALC5677=y
CONFIG_DRIVER_EC_CROS=y
CONFIG_DRIVER_EC_CROS_LPC=y
CONFIG_DRIVER_FLASH_MEMMAPPED=y
CONFIG_DRIVER_GPIO_LYNXPOINT_LP=y
CONFIG_DRIVER_INPUT_PS2=y
CONFIG_DRIVER_POWER_PCH=y
CONFIG_DRIVER_SOUND_I2S=y
CONFIG_DRIVER_SOUND_ROUTE=y
CONFIG_DRIVER_SOUND_RT5677=y
CONFIG_DRIVER_TPM_LPC=y
CONFIG_DRIVER_VIDEO_INTEL_I915=y
