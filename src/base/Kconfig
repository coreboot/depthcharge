config TIMESTAMP_RAW
	bool "Use raw TSC timestamp in coreboot timestamp table"
	default ARCH_X86

config HEADLESS
	bool "Allow headless mode of operation"
	default n
	help
	  "Set to 'y' for devices without display screens"
