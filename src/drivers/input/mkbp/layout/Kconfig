choice
	prompt "MKBP keyboard layout"
	depends on DRIVER_INPUT_MKBP
	default DRIVER_INPUT_MKBP_LAYOUT_US
	help
	  Maps scancodes into ASCII values.

config DRIVER_INPUT_MKBP_LAYOUT_US
	bool "US"

config DRIVER_INPUT_MKBP_LAYOUT_DE
	bool "DE"

endchoice
